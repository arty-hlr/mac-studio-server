<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.ollama.gpumemory</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/<OLLAMA_USER>/mac-studio-server/scripts/set-gpu-memory.sh</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>StandardErrorPath</key>
    <string>/Users/<OLLAMA_USER>/mac-studio-server/logs/gpu-memory.err</string>
    <key>StandardOutPath</key>
    <string>/Users/<OLLAMA_USER>/mac-studio-server/logs/gpu-memory.log</string>
</dict>
</plist> 